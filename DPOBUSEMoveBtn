	if script.Parent.Parent.Parent.Stop.Value == 3 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 4
		stset["1"].Text = codes.Lines.thirteenth.Stations.Four
		stset["2"].Text = codes.Lines.thirteenth.Stations.Five
