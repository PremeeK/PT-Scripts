		stsetepis["2"].Text = codes.Lines.fourteenth.Stations.Twelve
		stsetepis["1"].Text = codes.Lines.fourteenth.Stations.Thirteen
		stsetdots.Text = codes.Lines.fourteenth.Stations.Ten
	end

	if script.Parent.Parent.Parent.Stop.Value == 8 and script.Parent.Parent.Parent.Line.Value == 14 then
		script.Parent.Parent.Parent.Stop.Value = 9
		stset["1"].Text = codes.Lines.fourteenth.Stations.Nine
		stset["2"].Text = codes.Lines.fourteenth.Stations.Ten
		stset["3"].Text = codes.Lines.fourteenth.Stations.Eleven
		stset["4"].Text = codes.Lines.fourteenth.Stations.Twelve
		stsetepis["4"].Text = codes.Lines.fourteenth.Stations.Nine
		stsetepis["3"].Text = codes.Lines.fourteenth.Stations.Ten
		stsetepis["2"].Text = codes.Lines.fourteenth.Stations.Eleven
		stsetepis["1"].Text = codes.Lines.fourteenth.Stations.Twelve
		stsetdots.Text = codes.Lines.fourteenth.Stations.Nine
	end

	if script.Parent.Parent.Parent.Stop.Value == 7 and script.Parent.Parent.Parent.Line.Value == 14 then
		script.Parent.Parent.Parent.Stop.Value = 8
		stset["1"].Text = codes.Lines.fourteenth.Stations.Eight
		stset["2"].Text = codes.Lines.fourteenth.Stations.Nine
