--SIXTH--
script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.sixth.CountOfStops and script.Parent.Parent.Parent.Line.Value == 6 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.sixthline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["0"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["4"].Text = codes.Lines.sixth.Stations.Prazdno
		stset.Parent["L&D"].Linka.Text = codes.Lines.sixth.Stations.Prazdno
		stset.Parent["L&D"].Destinace.Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo.Parent["L&D"].Linka.Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo.Parent["L&D"].Destinace.Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.sixth.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetout.D.Text = codes.Lines.sixth.Stations.Prazdno
		stsetout.L.Text = codes.Lines.sixth.Stations.Prazdno
		stsetside.D.Text = codes.Lines.sixth.Stations.Prazdno
		stsetside.L.Text = codes.Lines.sixth.Stations.Prazdno
		stsetnum.TextLabel.Text = codes.Lines.sixth.Stations.Prazdno
		fronttexture.ImageLabel.Image = "rbxassetid://0"
		fronttexture.Enabled = false
		sidetexture.ImageLabel.Image = "rbxassetid://0"
		sidetexture.Enabled = false
		linetexture.ImageLabel.Image = "rbxassetid://0"
		linetexture.Enabled = false
	end

	if script.Parent.Parent.Parent.Stop.Value == 14 and script.Parent.Parent.Parent.Line.Value == 6 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.sixthline.FourteenFifteen
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = true
		stsetdots.Parent.Parent.TS["2"].Text = codes.Lines.sixth.Stations.Fourteen
		wait(3)
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		script.Parent.Parent.Parent.Stop.Value = 15
		stset["0"].Text = codes.Lines.sixth.Stations.Fifteen
		stset["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["4"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.sixth.Stations.Fifteen
		stsettwo["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.sixth.Stations.Fifteen
		stsetepis["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.sixth.Stations.Prazdno
		stsetdots.Text = codes.Lines.sixth.Stations.Fifteen
	end

	if script.Parent.Parent.Parent.Stop.Value == 13 and script.Parent.Parent.Parent.Line.Value == 6 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.sixthline.ThirteenFourteen
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = true
		stsetdots.Parent.Parent.TS["2"].Text = codes.Lines.sixth.Stations.Thirteen
		wait(3)
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		script.Parent.Parent.Parent.Stop.Value = 14
		stset["0"].Text = codes.Lines.sixth.Stations.Fourteen
		stset["1"].Text = codes.Lines.sixth.Stations.Fifteen
		stset["2"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["3"].Text = codes.Lines.sixth.Stations.Prazdno
		stset["4"].Text = codes.Lines.sixth.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.sixth.Stations.Fourteen
		stsettwo["1"].Text = codes.Lines.sixth.Stations.Fifteen
		stsettwo["2"].Text = codes.Lines.sixth.Stations.Prazdno
