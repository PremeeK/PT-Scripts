--THIRD--
script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.third.CountOfStops and script.Parent.Parent.Parent.Line.Value == 3 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.thirdline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["0"].Text = codes.Lines.third.Stations.Prazdno
		stset["1"].Text = codes.Lines.third.Stations.Prazdno
		stset["2"].Text = codes.Lines.third.Stations.Prazdno
		stset["3"].Text = codes.Lines.third.Stations.Prazdno
		stset["4"].Text = codes.Lines.third.Stations.Prazdno
		stset.Parent["L&D"].Linka.Text = codes.Lines.third.Stations.Prazdno
		stset.Parent["L&D"].Destinace.Text = codes.Lines.third.Stations.Prazdno
		stsettwo.Parent["L&D"].Linka.Text = codes.Lines.third.Stations.Prazdno
		stsettwo.Parent["L&D"].Destinace.Text = codes.Lines.third.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["1"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.third.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.third.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.third.Stations.Prazdno
		stsetout.D.Text = codes.Lines.third.Stations.Prazdno
		stsetout.L.Text = codes.Lines.third.Stations.Prazdno
		stsetside.D.Text = codes.Lines.third.Stations.Prazdno
		stsetside.L.Text = codes.Lines.third.Stations.Prazdno
		stsetnum.TextLabel.Text = codes.Lines.third.Stations.Prazdno
		fronttexture.ImageLabel.Image = "rbxassetid://0"
		fronttexture.Enabled = false
		sidetexture.ImageLabel.Image = "rbxassetid://0"
		sidetexture.Enabled = false
		linetexture.ImageLabel.Image = "rbxassetid://0"
		linetexture.Enabled = false
	end

	if script.Parent.Parent.Parent.Stop.Value == 14 and script.Parent.Parent.Parent.Line.Value == 3 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.thirdline.FourteenFifteen
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = true
		stsetdots.Parent.Parent.TS["2"].Text = codes.Lines.third.Stations.Fourteen
		wait(3)
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		script.Parent.Parent.Parent.Stop.Value = 15
		stset["0"].Text = codes.Lines.third.Stations.Fifteen
		stset["1"].Text = codes.Lines.third.Stations.Prazdno
		stset["2"].Text = codes.Lines.third.Stations.Prazdno
		stset["3"].Text = codes.Lines.third.Stations.Prazdno
		stset["4"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.third.Stations.Fifteen
		stsettwo["1"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.third.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.third.Stations.Fifteen
		stsetepis["3"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.third.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.third.Stations.Prazdno
		stsetdots.Text = codes.Lines.third.Stations.Fifteen
	end

	if script.Parent.Parent.Parent.Stop.Value == 13 and script.Parent.Parent.Parent.Line.Value == 3 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.thirdline.ThirteenFourteen
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = true
		stsetdots.Parent.Parent.TS["2"].Text = codes.Lines.third.Stations.Thirteen
		wait(3)
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		script.Parent.Parent.Parent.Stop.Value = 14
		stset["0"].Text = codes.Lines.third.Stations.Fourteen
		stset["1"].Text = codes.Lines.third.Stations.Fifteen
		stset["2"].Text = codes.Lines.third.Stations.Prazdno
		stset["3"].Text = codes.Lines.third.Stations.Prazdno
