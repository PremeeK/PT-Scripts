local codes = require(workspace["P&T's BUSE - EPIS"].Configuration)
-- local PANELS --
local stset = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky
local stsettwo = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSETWO["Obrazovka/Screen"].SurfaceGui.Zastavky
local stsetout = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs
local stsetside = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs
local stsetdots = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NS["2"]
local stsetepis = script.Parent.Parent.Parent.Stops
local stsetnum = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI
-- local TEXTURES --
local fronttexture = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Textury
local sidetexture = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.Textury
local linetexture = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.Textury
-- local PROPERTIES --
local reprak = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni
local Parcel = require(9428572121)
--
local beep = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Svetylka.On.beep
local beeptwo = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Svetylka.On.beeptwo

script.Parent.MouseButton1Up:Connect(function()
	beep:Play()
end)
--FIRST--
script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.first.CountOfStops and script.Parent.Parent.Parent.Line.Value == 1 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.firstline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["0"].Text = codes.Lines.first.Stations.Prazdno
		stset["1"].Text = codes.Lines.first.Stations.Prazdno
		stset["2"].Text = codes.Lines.first.Stations.Prazdno
		stset["3"].Text = codes.Lines.first.Stations.Prazdno
		stset["4"].Text = codes.Lines.first.Stations.Prazdno
		stset.Parent["L&D"].Linka.Text = codes.Lines.first.Stations.Prazdno
		stset.Parent["L&D"].Destinace.Text = codes.Lines.first.Stations.Prazdno
		stsettwo.Parent["L&D"].Linka.Text = codes.Lines.first.Stations.Prazdno
		stsettwo.Parent["L&D"].Destinace.Text = codes.Lines.first.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.first.Stations.Prazdno
		stsettwo["1"].Text = codes.Lines.first.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.first.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.first.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.first.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.first.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.first.Stations.Prazdno
		stsetout.D.Text = codes.Lines.first.Stations.Prazdno
		stsetout.L.Text = codes.Lines.first.Stations.Prazdno
		stsetside.D.Text = codes.Lines.first.Stations.Prazdno
		stsetside.L.Text = codes.Lines.first.Stations.Prazdno
		stsetnum.TextLabel.Text = codes.Lines.first.Stations.Prazdno
		fronttexture.ImageLabel.Image = "rbxassetid://0"
		fronttexture.Enabled = false
		sidetexture.ImageLabel.Image = "rbxassetid://0"
		sidetexture.Enabled = false
		linetexture.ImageLabel.Image = "rbxassetid://0"
		linetexture.Enabled = false
	end

	if script.Parent.Parent.Parent.Stop.Value == 14 and script.Parent.Parent.Parent.Line.Value == 1 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.firstline.FourteenFifteen
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
