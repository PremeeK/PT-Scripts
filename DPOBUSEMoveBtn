		stset["3"].Text = codes.Lines.twelfth.Stations.Seven
		stset["4"].Text = codes.Lines.twelfth.Stations.Eight
		stsetepis["4"].Text = codes.Lines.twelfth.Stations.Five
		stsetepis["3"].Text = codes.Lines.twelfth.Stations.Six
		stsetepis["2"].Text = codes.Lines.twelfth.Stations.Seven
		stsetepis["1"].Text = codes.Lines.twelfth.Stations.Eight
		stsetdots.Text = codes.Lines.twelfth.Stations.Five
	end

	if script.Parent.Parent.Parent.Stop.Value == 3 and script.Parent.Parent.Parent.Line.Value == 12 then
		script.Parent.Parent.Parent.Stop.Value = 4
		stset["1"].Text = codes.Lines.twelfth.Stations.Four
		stset["2"].Text = codes.Lines.twelfth.Stations.Five
		stset["3"].Text = codes.Lines.twelfth.Stations.Six
		stset["4"].Text = codes.Lines.twelfth.Stations.Seven
		stsetepis["4"].Text = codes.Lines.twelfth.Stations.Four
		stsetepis["3"].Text = codes.Lines.twelfth.Stations.Five
		stsetepis["2"].Text = codes.Lines.twelfth.Stations.Six
		stsetepis["1"].Text = codes.Lines.twelfth.Stations.Seven
		stsetdots.Text = codes.Lines.twelfth.Stations.Four
	end

	if script.Parent.Parent.Parent.Stop.Value == 2 and script.Parent.Parent.Parent.Line.Value == 12 then
		script.Parent.Parent.Parent.Stop.Value = 3
		stset["1"].Text = codes.Lines.twelfth.Stations.Three
		stset["2"].Text = codes.Lines.twelfth.Stations.Four
		stset["3"].Text = codes.Lines.twelfth.Stations.Five
		stset["4"].Text = codes.Lines.twelfth.Stations.Six
		stsetepis["4"].Text = codes.Lines.twelfth.Stations.Three
		stsetepis["3"].Text = codes.Lines.twelfth.Stations.Four
		stsetepis["2"].Text = codes.Lines.twelfth.Stations.Five
		stsetepis["1"].Text = codes.Lines.twelfth.Stations.Six
		stsetdots.Text = codes.Lines.twelfth.Stations.Three
	end

	if script.Parent.Parent.Parent.Stop.Value == 1 and script.Parent.Parent.Parent.Line.Value == 12 then
		script.Parent.Parent.Parent.Stop.Value = 2
		stset["1"].Text = codes.Lines.twelfth.Stations.Two
		stset["2"].Text = codes.Lines.twelfth.Stations.Three
		stset["3"].Text = codes.Lines.twelfth.Stations.Four
		stset["4"].Text = codes.Lines.twelfth.Stations.Five
		stsetepis["4"].Text = codes.Lines.twelfth.Stations.Two
		stsetepis["3"].Text = codes.Lines.twelfth.Stations.Three
		stsetepis["2"].Text = codes.Lines.twelfth.Stations.Four
		stsetepis["1"].Text = codes.Lines.twelfth.Stations.Five
		stsetdots.Text = codes.Lines.twelfth.Stations.Two
	end

end)

-- THIRTEENTH --

script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.thirteenth.CountOfStops and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.thirteenthline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stset.Parent.Ostatni.Konecna.Visible = true
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["1"].Text = codes.Lines.first.Stations.Prazdno
		stset["2"].Text = codes.Lines.first.Stations.Prazdno
		stset["3"].Text = codes.Lines.first.Stations.Prazdno
		stset["4"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.first.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.first.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.first.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.first.Stations.Prazdno
		stsetout.D.Text = codes.Lines.first.Stations.Prazdno
		stsetout.L.Text = codes.Lines.first.Stations.Prazdno
		stsetside.D.Text = codes.Lines.first.Stations.Prazdno
		stsetside.L.Text = codes.Lines.first.Stations.Prazdno
		stsetnum.TextLabel.Text = codes.Lines.first.Stations.Prazdno
		fronttexture.ImageLabel.Image = "rbxassetid://0"
		fronttexture.Enabled = false
		sidetexture.ImageLabel.Image = "rbxassetid://0"
		sidetexture.Enabled = false
		linetexture.ImageLabel.Image = "rbxassetid://0"
		linetexture.Enabled = false
	end

	if script.Parent.Parent.Parent.Stop.Value == 14 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 15
		stset["1"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stset["2"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stset["3"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stset["4"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetdots.Text = codes.Lines.thirteenth.Stations.Fifteen
	end

	if script.Parent.Parent.Parent.Stop.Value == 13 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 14
		stset["1"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stset["2"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stset["3"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stset["4"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetdots.Text = codes.Lines.thirteenth.Stations.Fourteen
	end

	if script.Parent.Parent.Parent.Stop.Value == 12 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 13
		stset["1"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stset["2"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stset["3"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stset["4"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Prazdno
		stsetdots.Text = codes.Lines.thirteenth.Stations.Thirteen
	end

	if script.Parent.Parent.Parent.Stop.Value == 11 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 12
		stset["1"].Text = codes.Lines.thirteenth.Stations.Twelve
		stset["2"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stset["3"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stset["4"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Twelve
