end)


--SECOND--
script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.second.CountOfStops and script.Parent.Parent.Parent.Line.Value == 2 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.secondline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["0"].Text = codes.Lines.second.Stations.Prazdno
		stset["1"].Text = codes.Lines.second.Stations.Prazdno
		stset["2"].Text = codes.Lines.second.Stations.Prazdno
		stset["3"].Text = codes.Lines.second.Stations.Prazdno
		stset["4"].Text = codes.Lines.second.Stations.Prazdno
		stset.Parent["L&D"].Linka.Text = codes.Lines.second.Stations.Prazdno
		stset.Parent["L&D"].Destinace.Text = codes.Lines.second.Stations.Prazdno
		stsettwo.Parent["L&D"].Linka.Text = codes.Lines.second.Stations.Prazdno
		stsettwo.Parent["L&D"].Destinace.Text = codes.Lines.second.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.second.Stations.Prazdno
		stsettwo["1"].Text = codes.Lines.second.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.second.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.second.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.second.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.second.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.second.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.second.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.second.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.second.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.second.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.second.Stations.Prazdno
		stsetout.D.Text = codes.Lines.second.Stations.Prazdno
		stsetout.L.Text = codes.Lines.second.Stations.Prazdno
		stsetside.D.Text = codes.Lines.second.Stations.Prazdno
		stsetside.L.Text = codes.Lines.second.Stations.Prazdno
		stsetnum.TextLabel.Text = codes.Lines.second.Stations.Prazdno
		fronttexture.ImageLabel.Image = "rbxassetid://0"
		fronttexture.Enabled = false
		sidetexture.ImageLabel.Image = "rbxassetid://0"
		sidetexture.Enabled = false
		linetexture.ImageLabel.Image = "rbxassetid://0"
		linetexture.Enabled = false
	end

	if script.Parent.Parent.Parent.Stop.Value == 14 and script.Parent.Parent.Parent.Line.Value == 2 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.secondline.FourteenFifteen
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
