	end
end)

--FIFTH--
script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.fifth.CountOfStops and script.Parent.Parent.Parent.Line.Value == 5 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.fifthline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["0"].Text = codes.Lines.fifth.Stations.Prazdno
		stset["1"].Text = codes.Lines.fifth.Stations.Prazdno
		stset["2"].Text = codes.Lines.fifth.Stations.Prazdno
		stset["3"].Text = codes.Lines.fifth.Stations.Prazdno
		stset["4"].Text = codes.Lines.fifth.Stations.Prazdno
		stset.Parent["L&D"].Linka.Text = codes.Lines.fifth.Stations.Prazdno
		stset.Parent["L&D"].Destinace.Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo.Parent["L&D"].Linka.Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo.Parent["L&D"].Destinace.Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo["1"].Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.fifth.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.fifth.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.fifth.Stations.Prazdno
		stsetout.D.Text = codes.Lines.fifth.Stations.Prazdno
