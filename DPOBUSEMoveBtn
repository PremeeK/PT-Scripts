		stsettwo["4"].Text = codes.Lines.sixth.Stations.Six
		stsetepis["4"].Text = codes.Lines.sixth.Stations.Two
		stsetepis["3"].Text = codes.Lines.sixth.Stations.Three
		stsetepis["2"].Text = codes.Lines.sixth.Stations.Four
		stsetepis["1"].Text = codes.Lines.sixth.Stations.Five
		stsetdots.Text = codes.Lines.sixth.Stations.Two

	end
end)

--SEVENTH--
script.Parent.MouseButton1Up:Connect(function()
	if script.Parent.Parent.Parent.Stop.Value == 15 or script.Parent.Parent.Parent.Stop.Value == codes.Lines.seventh.CountOfStops and script.Parent.Parent.Parent.Line.Value == 7 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.seventhline.FifteenLASTSTOP
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
		-- disabling script + showing last stop --
		stsetdots.Parent.Parent.Script.Enabled = false
		stsetdots.Parent.Parent.NL.Visible = false
		stsetdots.Parent.Parent.NS.Visible = false
		stsetdots.Parent.Parent.Cas.Visible = false
		stsetdots.Parent.Parent.NexSto.Visible = false
		stsetdots.Parent.Parent.TS.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.KonZast.Visible = true
		wait(2)
		stsetdots.Parent.Parent.KonZast.Visible = false
		stsetdots.Parent.Parent.VysPros.Visible = true
		wait(2)
		stsetdots.Parent.Parent.VysPros.Visible = false
		stsetdots.Parent.Parent.Script.Enabled = true
		-- done --
		script.Parent.Parent.Parent.Stop.Value = 0
		script.Parent.Parent.Parent.Line.Value = 0
		stset["0"].Text = codes.Lines.seventh.Stations.Prazdno
		stset["1"].Text = codes.Lines.seventh.Stations.Prazdno
		stset["2"].Text = codes.Lines.seventh.Stations.Prazdno
		stset["3"].Text = codes.Lines.seventh.Stations.Prazdno
		stset["4"].Text = codes.Lines.seventh.Stations.Prazdno
