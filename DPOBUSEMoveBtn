	end

	if script.Parent.Parent.Parent.Stop.Value == 1 and script.Parent.Parent.Parent.Line.Value == 4 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.fourthline.OneTwo
