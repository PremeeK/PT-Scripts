		stsetepis["3"].Text = codes.Lines.fourth.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.fourth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.fourth.Stations.Prazdno
