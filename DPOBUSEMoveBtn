		stsetepis["3"].Text = codes.Lines.fourth.Stations.Six
		stsetepis["2"].Text = codes.Lines.fourth.Stations.Seven
		stsetepis["1"].Text = codes.Lines.fourth.Stations.Eight
		stsetdots.Text = codes.Lines.fourth.Stations.Five
	end

	if script.Parent.Parent.Parent.Stop.Value == 3 and script.Parent.Parent.Parent.Line.Value == 4 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.fourthline.ThreeFour
