	if script.Parent.Parent.Parent.Stop.Value == 6 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 7
		stset["1"].Text = codes.Lines.thirteenth.Stations.Seven
		stset["2"].Text = codes.Lines.thirteenth.Stations.Eight
		stset["3"].Text = codes.Lines.thirteenth.Stations.Nine
