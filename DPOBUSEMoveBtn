		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Fifteen
		stsetdots.Text = codes.Lines.thirteenth.Stations.Twelve
	end

	if script.Parent.Parent.Parent.Stop.Value == 10 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 11
		stset["1"].Text = codes.Lines.thirteenth.Stations.Eleven
		stset["2"].Text = codes.Lines.thirteenth.Stations.Twelve
		stset["3"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stset["4"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Eleven
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Twelve
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Fourteen
		stsetdots.Text = codes.Lines.thirteenth.Stations.Eleven
	end

	if script.Parent.Parent.Parent.Stop.Value == 9 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 10
		stset["1"].Text = codes.Lines.thirteenth.Stations.Ten
		stset["2"].Text = codes.Lines.thirteenth.Stations.Eleven
		stset["3"].Text = codes.Lines.thirteenth.Stations.Twelve
		stset["4"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Ten
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Eleven
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Twelve
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Thirteen
		stsetdots.Text = codes.Lines.thirteenth.Stations.Ten
	end

	if script.Parent.Parent.Parent.Stop.Value == 8 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 9
		stset["1"].Text = codes.Lines.thirteenth.Stations.Nine
		stset["2"].Text = codes.Lines.thirteenth.Stations.Ten
		stset["3"].Text = codes.Lines.thirteenth.Stations.Eleven
		stset["4"].Text = codes.Lines.thirteenth.Stations.Twelve
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Nine
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Ten
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Eleven
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Twelve
		stsetdots.Text = codes.Lines.thirteenth.Stations.Nine
	end

	if script.Parent.Parent.Parent.Stop.Value == 7 and script.Parent.Parent.Parent.Line.Value == 13 then
		script.Parent.Parent.Parent.Stop.Value = 8
		stset["1"].Text = codes.Lines.thirteenth.Stations.Eight
		stset["2"].Text = codes.Lines.thirteenth.Stations.Nine
