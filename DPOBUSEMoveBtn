		fronttexture.ImageLabel.Image = "rbxassetid://0"
		fronttexture.Enabled = false
		sidetexture.ImageLabel.Image = "rbxassetid://0"
		sidetexture.Enabled = false
		linetexture.ImageLabel.Image = "rbxassetid://0"
		linetexture.Enabled = false
	end

	if script.Parent.Parent.Parent.Stop.Value == 14 and script.Parent.Parent.Parent.Line.Value == 8 then
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.eighthline.FourteenFifteen
