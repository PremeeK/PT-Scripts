		stsettwo.Parent["L&D"].Destinace.Text = codes.Lines.eighth.Stations.Prazdno
		stsettwo["0"].Text = codes.Lines.eighth.Stations.Prazdno
		stsettwo["1"].Text = codes.Lines.eighth.Stations.Prazdno
		stsettwo["2"].Text = codes.Lines.eighth.Stations.Prazdno
		stsettwo["3"].Text = codes.Lines.eighth.Stations.Prazdno
		stsettwo["4"].Text = codes.Lines.eighth.Stations.Prazdno
		stsetepis["4"].Text = codes.Lines.eighth.Stations.Prazdno
		stsetepis["3"].Text = codes.Lines.eighth.Stations.Prazdno
		stsetepis["2"].Text = codes.Lines.eighth.Stations.Prazdno
		stsetepis["1"].Text = codes.Lines.eighth.Stations.Prazdno
		stsetepis.Dest.Text = 'Použij tlačítko "VYPNOUT" pro odhlášení.'
		stsetepis.Line.Text = '->'
		stsetdots.Text = codes.Lines.eighth.Stations.Prazdno
		stsetdots.Parent.Parent.NL["1"].Text = codes.Lines.eighth.Stations.Prazdno
		stsetdots.Parent.Parent.NL["2"].Text = codes.Lines.eighth.Stations.Prazdno
