		stset["4"].Text = codes.Lines.thirteenth.Stations.Seven
		stsetepis["4"].Text = codes.Lines.thirteenth.Stations.Four
		stsetepis["3"].Text = codes.Lines.thirteenth.Stations.Five
		stsetepis["2"].Text = codes.Lines.thirteenth.Stations.Six
		stsetepis["1"].Text = codes.Lines.thirteenth.Stations.Seven
