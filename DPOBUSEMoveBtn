		script.Parent.Parent.Parent.Stop.Value = 6
		stset["1"].Text = codes.Lines.thirteenth.Stations.Six
		stset["2"].Text = codes.Lines.thirteenth.Stations.Seven
		stset["3"].Text = codes.Lines.thirteenth.Stations.Eight
