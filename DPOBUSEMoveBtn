		script.Parent.Parent.Parent.Stop.Value = 5
		stset["1"].Text = codes.Lines.thirteenth.Stations.Five
		stset["2"].Text = codes.Lines.thirteenth.Stations.Six
		stset["3"].Text = codes.Lines.thirteenth.Stations.Seven
