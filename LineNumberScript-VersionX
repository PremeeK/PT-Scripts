		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.D.Text = codes.Lines.fifteenth.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.TextLabel.Text = codes.Lines.fifteenth.Name
		--textures--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Textury.ImageLabel.Image = codes.Lines.fifteenth.Textura
