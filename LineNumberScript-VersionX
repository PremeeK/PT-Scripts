		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.L.Text = codes.Lines.third.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.D.Text = codes.Lines.third.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.TextLabel.Text = codes.Lines.third.Name
