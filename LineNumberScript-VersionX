		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["1"].Text = codes.Lines.third.Stations.One
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["2"].Text = codes.Lines.third.Stations.Two
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["3"].Text = codes.Lines.third.Stations.Three
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["4"].Text = codes.Lines.third.Stations.Four
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Stop.Value = 1
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Line.Value = 3
		--showing the dest, line & next stop on the buse --
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["1"].Text = codes.Lines.third.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["2"].Text = codes.Lines.third.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NS["2"].Text = codes.Lines.third.Stations.One
		--showing the dest & line on the outside panels--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs.L.Text = codes.Lines.third.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs.D.Text = codes.Lines.third.Dest
