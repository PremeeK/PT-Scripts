		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.Enabled = false
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.fifteenthline.OneTwo
		--setitng text back to Enter the code...--
		script.Parent.Parent.Parent.CodeEnter.Text = "Enter the code.."
		--hlaseni--
		wait(5)
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.fifteenthline.BeforeRunDriverAnnouncement
