local codes = require(workspace["P&T's BUSE - EPIS"].Configuration)
local beep = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Svetylka.On.beep
script.Parent.MouseButton1Up:Connect(function()
	beep:Play()
	if codes.Lines.first.Login == script.Parent.Parent.Parent.CodeEnter.Text then
		--setting the dest and the name--
		script.Parent.Parent.Parent.Parent.Main.Stops.Dest.Text = codes.Lines.first.Dest 
		script.Parent.Parent.Parent.Parent.Main.Stops.Line.Text = codes.Lines.first.Name
		--disabling and enabling visibility of the guis--
		script.Parent.Parent.Parent.Parent.LineNumber.Visible = false
		script.Parent.Parent.Parent.Parent.Main.Visible = true
		--showing the stops on the epis--
		script.Parent.Parent.Parent.Parent.Main.Stops["4"].Text = codes.Lines.first.Stations.One
		script.Parent.Parent.Parent.Parent.Main.Stops["3"].Text = codes.Lines.first.Stations.Two
		script.Parent.Parent.Parent.Parent.Main.Stops["2"].Text = codes.Lines.first.Stations.Three
		script.Parent.Parent.Parent.Parent.Main.Stops["1"].Text = codes.Lines.first.Stations.Four
		--setting the check on--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Script.Disabled = true
		--setting the dest and the line on buse--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui["L&D"].Linka.Text = codes.Lines.first.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui["L&D"].Destinace.Text = codes.Lines.first.Dest
		--showing the stops on the buse--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["1"].Text = codes.Lines.first.Stations.One
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["2"].Text = codes.Lines.first.Stations.Two
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["3"].Text = codes.Lines.first.Stations.Three
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["4"].Text = codes.Lines.first.Stations.Four
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Stop.Value = 1
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Line.Value = 1
		--showing the dest, line & next stop on the buse--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["1"].Text = codes.Lines.first.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["2"].Text = codes.Lines.first.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NS["2"].Text = codes.Lines.first.Stations.One
		--showing the dest & line on the outside panels--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs.L.Text = codes.Lines.first.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs.D.Text = codes.Lines.first.Dest
		--showing the dest & line on the outside side panels--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.L.Text = codes.Lines.first.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.D.Text = codes.Lines.first.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.TextLabel.Text = codes.Lines.first.Name
		--textures--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Textury.ImageLabel.Image = codes.Lines.first.Textura
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.Textury.ImageLabel.Image = codes.Lines.first.TexturaBocni
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.Textury.ImageLabel.Image = codes.Lines.first.TexturaLinka
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.Enabled = false
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.firstline.OneTwo
		--setitng text back to Enter the code...--
		script.Parent.Parent.Parent.CodeEnter.Text = "Enter the code.."
		wait(5)
		--hlaseni--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.firstline.BeforeRunDriverAnnouncement
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
	end
	if codes.Lines.second.Login == script.Parent.Parent.Parent.CodeEnter.Text then
		--setting the dest and the name--
		script.Parent.Parent.Parent.Parent.Main.Stops.Dest.Text = codes.Lines.second.Dest 
		script.Parent.Parent.Parent.Parent.Main.Stops.Line.Text = codes.Lines.second.Name
		--disabling and enabling visibility of the guis--
		script.Parent.Parent.Parent.Parent.LineNumber.Visible = false
		script.Parent.Parent.Parent.Parent.Main.Visible = true
		--showing the stops on the epis--
		script.Parent.Parent.Parent.Parent.Main.Stops["4"].Text = codes.Lines.second.Stations.One
		script.Parent.Parent.Parent.Parent.Main.Stops["3"].Text = codes.Lines.second.Stations.Two
		script.Parent.Parent.Parent.Parent.Main.Stops["2"].Text = codes.Lines.second.Stations.Three
		script.Parent.Parent.Parent.Parent.Main.Stops["1"].Text = codes.Lines.second.Stations.Four
		--setting the check on--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Script.Disabled = true
		--setting the dest and the line on buse--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui["L&D"].Linka.Text = codes.Lines.second.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui["L&D"].Destinace.Text = codes.Lines.second.Dest
		--showing the stops on the buse--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["1"].Text = codes.Lines.second.Stations.One
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["2"].Text = codes.Lines.second.Stations.Two
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["3"].Text = codes.Lines.second.Stations.Three
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.BUSE["Obrazovka/Screen"].SurfaceGui.Zastavky["4"].Text = codes.Lines.second.Stations.Four
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Stop.Value = 1
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Line.Value = 2
		--showing the dest, line & next stop on the buse--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["1"].Text = codes.Lines.second.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["2"].Text = codes.Lines.second.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NS["2"].Text = codes.Lines.second.Stations.One
		--showing the dest & line on the outside panels--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs.L.Text = codes.Lines.second.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.GUIs.D.Text = codes.Lines.second.Dest
		--showing the dest & line on the outside side panels--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.L.Text = codes.Lines.second.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.GUIs.D.Text = codes.Lines.second.Dest
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.TextLabel.Text = codes.Lines.second.Name
		--textures--
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Textury.ImageLabel.Image = codes.Lines.second.Textura
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSE.Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.Textury.ImageLabel.Image = codes.Lines.second.TexturaBocni
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.OUTSIDEBUSESIDE.Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.Textury.ImageLabel.Image = codes.Lines.second.TexturaLinka
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.Textury.Enabled = true
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent["NUMBER BUSE"].Screen.GUI.Enabled = false
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.secondline.OneTwo
		--setitng text back to Enter the code...--
		script.Parent.Parent.Parent.CodeEnter.Text = "Enter the code.."
		--hlaseni--
		wait(5)
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound.SoundId = codes.Lines.StationAnnouncements.secondline.BeforeRunDriverAnnouncement
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.REPRODUKTOR.RozlehlostHlaseni.Sound:Play()
	end
	if codes.Lines.third.Login == script.Parent.Parent.Parent.CodeEnter.Text then
		--setting the dest and the line on epis--
		script.Parent.Parent.Parent.Parent.Main.Stops.Dest.Text = codes.Lines.third.Dest
		script.Parent.Parent.Parent.Parent.Main.Stops.Line.Text = codes.Lines.third.Name
