		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Stop.Value = 1
		script.Parent.Parent.Parent.Parent.Parent.Screen.Main.Line.Value = 15
		--showing the dest, line & next stop on the buse --
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["1"].Text = codes.Lines.fifteenth.Name
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.DOTTEDBUSE.Screen.GUIs.NL["2"].Text = codes.Lines.fifteenth.Dest
